{
  "name": "strive-molu-axios",
  "packageManager": "pnpm@8.15.5",
  "version": "0.0.3-beta.11",
  "description": "基于 axios 封装，具有接口防抖，失败重新请求等功能。",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "keywords": [
    "axios",
    "strive-molu"
  ],
  "author": "",
  "license": "ISC",
  "exports": {
    ".": {
      "types": {
        "require": "./dist/index.d.cts",
        "import": "./dist/index.d.ts"
      },
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhoubaos/strive-molu-axios.git"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint .",
    "lint:fix": "pnpm run lint --fix",
    "tag": "tsx bin/zx/tag.ts",
    "typecheck": "tsc --noEmit",
    "typecheck:client": "pnpm -C examples/client run typecheck",
    "client:dev": "pnpm -C examples/client run dev",
    "client:build": "pnpm -C examples/client run build",
    "server": "nodemon --exec tsx examples/server/index.ts"
  },
  "peerDependencies": {
    "axios": "^1.7.2"
  },
  "dependencies": {
    "pako": "^2.1.0",
    "ts-md5": "^1.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@types/pako": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "chalk": "^5.3.0",
    "consola": "^3.2.3",
    "defu": "^6.1.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-markdown": "^5.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.26.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.2.5",
    "tsup": "^8.5.0",
    "tsx": "^4.11.2",
    "typescript": "^5.4.5",
    "yaml-eslint-parser": "^1.2.3",
    "zx": "^8.1.2"
  },
  "engines": {
    "node": "16.x || >=18.x"
  }
}
